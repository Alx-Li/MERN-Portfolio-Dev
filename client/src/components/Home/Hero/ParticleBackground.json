{
    "background": {
      "color": {
        "value": "#f9f9f9"
      },
      "position": "fixed",
      "repeat": "no-repeat",
      "size": "cover"
    },
    "fullScreen": {
      "enable": true,
      "zIndex": -1
    },
    "fpsLimit": 60,
