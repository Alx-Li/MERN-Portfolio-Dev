      },
      "modes": {
        "bubble": {
          "distance": 400,
          "duration": 2,
          "opacity": 0.8,
          "size": 40
        },
        "grab": {
          "distance": 400
        }
      }
    },
    "motion": {
      "disable": false,
      "reduce": {
        "value": false
      }
    },
    "particles": {
      "color": {
        "value": "#cfcfcf"
      },
      "links": {
        "color": {
          "value": "#cfcfcf"
        },
        "distance": 150,
        "enable": true,
        "opacity": 0.4
      },
      "move": {
        "attract": {
          "rotate": {
            "x": 600,
            "y": 1200
          }
        },
        "enable": true,
        "path": {},
        "outModes": {
          "bottom": "out",
          "left": "out",
          "right": "out",
          "top": "out"
        },
        "speed": 0.5,
        "spin": {}
      },
      "number": {
        "density": {
          "enable": true,
          "area": 1000
        }
      },
      "opacity": {
        "random": {
          "enable": true
        },
        "value": {
          "min": 0.1,
          "max": 1
        },
        "animation": {
          "speed": 3,
          "minimumValue": 0.1
